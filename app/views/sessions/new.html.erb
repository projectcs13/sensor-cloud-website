<% provide(:title, "Sign in") %>
<h1>Sign in</h1>
<div class="row">
	<div class="col-md-6 col-md-offset-3 login-form">
		<style>

		</style>
		<%= render 'tos' %>

		<div id="gConnect" style="display:none;">
			<p class="left">Sign in using Google's Open ID Connect button</p>
			<div class="signin-btn">
		    <button class="g-signin"
	        data-scope="https://www.googleapis.com/auth/plus.login https://www.googleapis.com/auth/userinfo.email"
	        data-clientId="995342763478-fh8bd2u58n1tl98nmec5jrd76dkbeksq.apps.googleusercontent.com"
	        data-callback="signinCallback"
	        data-accesstype="offline"
	        data-approvalprompt="force"
	        data-cookiepolicy="single_host_origin">
		    </button>
			</div>
	  </div>
	</div>
</div>
